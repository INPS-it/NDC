[] a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf CLV:hasCountry ] ;
            owl:someValuesFrom [ a owl:Restriction ;
                    owl:onProperty [ owl:inverseOf CLV:withAddress ] ;
                    owl:someValuesFrom [ a owl:Restriction ;
                            owl:onProperty TI:atTime ;
                            owl:someValuesFrom INPSPensions:PeriodoAttivitaEstera ] ] ] ;
    owl:intersectionOf (
            [ a owl:Class ;
                owl:unionOf (
                        INPSPensions:NazioneEsteraConvenzioneBilaterale
                        INPSPensions:NazioneEsteraConvenzioneUE ) ]
            [ a owl:Restriction ;
                owl:onProperty [ owl:inverseOf INPSPensions:haNazioneEstera ] ;
                owl:someValuesFrom owl:Thing ] ) .

[] a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf L0:hasMember ] ;
            owl:someValuesFrom INPSPensions:PrestazionePensionistica ] ;
    owl:unionOf (
            INPSPensions:DelegaRiscossioneContributiSindacali
            INPSPensions:DetrazioneDiImposta
            INPSPensions:Reddito
            INPSPensions:SedeDiCompetenzaDiPP
            INPSPensions:StatoDiCertificazionePP
            INPSPensions:StatoDiDomandaPP
            INPSPensions:StatoDiEliminazionePP
            INPSPensions:StatoDiFruizionePP
            INPSPensions:StatoDiIstruttoriaDiPP
            INPSPensions:UfficioPagatoreDiPP ) .

[] a owl:Class ;
    owl:disjointWith [ a owl:Class ;
            owl:oneOf (
                    INPSPensions:GestioneExINPDAP ) ] ;
    owl:oneOf (
            INPSPensions:Cumulo
            INPSPensions:Totalizzazione ) .

