    adms:hasSemanticAssetDistribution <https://w3id.org/italia/social-security/data/pensions/distribution/PENSIONS-N-TRIPLES>,
        <https://w3id.org/italia/social-security/data/pensions/distribution/PENSIONS-RDF-TURTLE>,
        <https://w3id.org/italia/social-security/data/pensions/distribution/PENSIONS-RDF-XML> ;
    adms:hasTask adms:AnnotationTask,
        adms:QueryFormulationTask,
        adms:SearchTask ;
    adms:last INPSPensions:1.0 ;
    adms:officialURI "https://w3id.org/italia/social-security/onto/pensions"^^xsd:anyURI ;
    adms:prefix "INPSPensions" ;
    adms:semanticAssetInUse <https://w3id.org/italia/data/project/NDC> ;
    adms:status "catalogued",
        "published" ;
    adms:target "Cittadini"@it,
        "Imprese"@it,
        "Pubbliche Amministrazioni"@it ;
    adms:type adms:ServiceOnytology ;
    dcat:contactPoint <https://w3id.org/italia/social-security/data/organization/DirezioneCentraleTII> ;
    dcat:keyword "Paying Office"@en,
        "Pension Benefit"@en,
        "Pension Product"@en,
        "Prestazione Pensionistica"@it,
        "Prodotto Pensionistico"@it,
        "Ufficio Pagatore"@it ;
    dcat:theme <http://publications.europa.eu/resource/authority/data-theme/SOCI> ;
    dct:accrualPeriodicity <http://publications.europa.eu/resource/authority/frequency/IRREG> ;
    dct:creator <https://w3id.org/italia/social-security/data/organization/inps> ;
    dct:description "Ontologia INPS relativa alle entità e alle relazioni che descrivono la prestazione pensionistica erogata dall'istituto. Tra di esse troviamo, ad esempio, i vari stati in cui si puo trovare la domanda o le diverse modalità di pagamento con cui INPS eroga la prestazione al cittadino."@it,
        "This is the ontology of the INPS Organization about entities and relationships that describe the pension benefit provided by the institution. These include, for example, the states in which the application can be found or the different payment methods wherewith INPS provides the service to the citizen."@en ;
    dct:identifier "https://w3id.org/italia/social-security/onto/pensions" ;
    dct:issued "2024-03-06T00:00:00+00:00"^^xsd:dateTime ;
