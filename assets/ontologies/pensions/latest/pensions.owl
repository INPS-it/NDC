                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/social-security/onto/pensions/ContoCorrenteBancario"/>
        <rdfs:subClassOf>
