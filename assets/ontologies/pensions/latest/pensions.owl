                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/CPV/belongsToFamily"/>
                                <owl:someValuesFrom rdf:resource="https://w3id.org/italia/social-security/onto/pensions/FamigliaDiTitolareDiPP"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith"/>
                                <owl:someValuesFrom>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="https://w3id.org/italia/onto/RO/holdsRoleInTime"/>
