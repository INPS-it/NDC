    <owl:NamedIndividual rdf:about="https://w3id.org/italia/social-security/onto/pensions/AnzianitaAnticipataVecchiaia">
        <rdf:type rdf:resource="https://w3id.org/italia/social-security/onto/pensions/GruppoServizioPensionistico"/>
        <rdfs:comment xml:lang="en">An instance of the Pension service group class that aggregates the services concerning the three pension forms mentioned.</rdfs:comment>
