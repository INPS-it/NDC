      </owl:Restriction>
    </owl:equivalentClass>

    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://w3id.org/italia/social-security/onto/pensionProspectusLavoratore/haTipologiaSistemaDiCalcolo"/>
