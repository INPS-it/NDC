        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
      </owl:Restriction>
    </owl:equivalentClass>

    <rdfs:comment xml:lang="it">La classe raggruppa le domande di pensione presentate all'INPS</rdfs:comment>
    <rdfs:comment xml:lang="en">The class groups the pension applications submitted to INPS</rdfs:comment>
    <rdfs:label xml:lang="it">Domanda di pensione</rdfs:label>
    <rdfs:label xml:lang="en">Pension application</rdfs:label>
  </owl:Class>

