    <rdfs:comment xml:lang="en">The class represents one of the two main information groups present in the pension determination document, relating to the details of the pension amounts paid by INPS to the Benefit Holder</rdfs:comment>
    <rdfs:label xml:lang="en">Amount of pension</rdfs:label>
    <rdfs:label xml:lang="it">Importo pensione</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/italia/social-security/onto/pensionProspectusLavoratore/Instante">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://w3id.org/italia/social-security/onto/pensionProspectusLavoratore/presentaDomandaDiPensione"/>
