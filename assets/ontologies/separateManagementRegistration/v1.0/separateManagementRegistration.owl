    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/domandaIscrizioneGestioneSeparataInviataA -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/domandaIscrizioneGestioneSeparataInviataA">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparata"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/core/SedeINPS"/>
        <rdfs:comment xml:lang="it">La object property collega la domanda di iscrizione alla gestione separata alla sede INPS di riferimento competente per il suo accoglimento e per la successiva lavorazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This object property links together the Spilt Scheme Application Form class and the INPS Service class. In fact, the Spilt Scheme is a service processed and managed by INPS offices. </rdfs:comment>
        <rdfs:label xml:lang="it">domandaIscrizioneGestioneSeparataInviataA</rdfs:label>
        <rdfs:label xml:lang="en">spiltSchemeApplicationFormSentTo</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/generaContributi -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/generaContributi">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/IscrizioneGestioneSeparata"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/social-safety-net/ContributoGestioneSeparata"/>
        <rdfs:comment xml:lang="it">La relazione connette tra loro le classi Iscrizione gestione separata e Contributo gestione separata per descrivere l&apos;obbligo di versamento dei contributi conseguente all&apos;iscrizione del lavoratore alla gestione previdenziale.</rdfs:comment>
        <rdfs:comment xml:lang="en">This object property links together the Spilt Scheme Registration class and the Spilt Scheme Contributions class. Once registered with the Spilt Scheme you must pay your credited contributions into the Scheme Account.  </rdfs:comment>
        <rdfs:label xml:lang="it">generaContributi</rdfs:label>
        <rdfs:label xml:lang="en">hasContributions</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/generaIscrizioneGestioneSeparata -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/generaIscrizioneGestioneSeparata">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparata"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/IscrizioneGestioneSeparata"/>
        <rdfs:comment xml:lang="it">La proprietà mette in relazione la Domanda di iscrizione alla gestione separata con l&apos;Iscrizione alla gestione separata per descrivere come la corretta compilazione  e l&apos;invio della domanda abbiano come effetto l&apos;iscrizione del lavoratore alla gestione separata</rdfs:comment>
        <rdfs:comment xml:lang="en">This object property links together the Spilt Scheme Application Form and the Spilt Scheme Registration class. Those classes are essentional to show all the stages of the application process. </rdfs:comment>
        <rdfs:label xml:lang="it">generaIscrizioneGestioneSeparata</rdfs:label>
        <rdfs:label xml:lang="en">hasSpiltSchemeRegistration</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaGestioneSeparataLiberoProfessionista -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaGestioneSeparataLiberoProfessionista">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaIscrizioneGestioneSeparata"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/social-safety-net/LiberoProfessionista"/>
        <rdfs:domain>
            <owl:Class>
                <owl:complementOf rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/LiberoProfessionistaProtettoConCassa"/>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparataLiberoProfessionista"/>
        <rdfs:comment xml:lang="it">La relazione collega il Libero professionista con obbligo di iscrizione alla gestione separata, nella veste di soggeto che invia la domadna di iscrizione, alla domanda di iscrizione alla gestione separata rivolta a questa tipologia di lavoratore</rdfs:comment>
        <rdfs:comment xml:lang="en">This object property links together the Spilt Scheme Self Employed class and the Self Employed Spilt Scheme Application Form. </rdfs:comment>
        <rdfs:label xml:lang="en">hasSelfEmployedSpiltSchemeApplicationForm</rdfs:label>
        <rdfs:label xml:lang="it">inviaDomandaGestioneSeparataLiberoProfessionista</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaGestioneSeparataParasubordinato -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaGestioneSeparataParasubordinato">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaIscrizioneGestioneSeparata"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/social-safety-net/LavoratoreParasubordinato"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparataParasubordinato"/>
        <rdfs:comment xml:lang="it">La relazione collega il lavoratore parasubordinato, nella veste di soggeto che invia la domadna di iscrizione, alla domanda di iscrizione alla gestione separata rivolta a questa tipologia di lavoratore</rdfs:comment>
        <rdfs:comment xml:lang="en">This object property links together the Dependent Contractor class and the Dependent Contractor Spilt Scheme Application Form.</rdfs:comment>
        <rdfs:label xml:lang="en">hasDependentContractorSpiltSchemeApplicationForm</rdfs:label>
        <rdfs:label xml:lang="it">inviaDomandaGestioneSeparataParasubordinato</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaIscrizioneGestioneSeparata -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/inviaDomandaIscrizioneGestioneSeparata">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/l0/Agent"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/BeneficiarioDomandaGestioneSeparata"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparata"/>
        <rdfs:label xml:lang="en">inviaDomandaIscrizioneGestioneSeparata</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/iscriveLavoratoreInGestioneSeparata -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/iscriveLavoratoreInGestioneSeparata">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/IscrizioneGestioneSeparata"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/GestioneSeparata"/>
        <rdfs:comment xml:lang="it">La relazione collega la classe Iscrizione gestione separata alla classe gestione separata per indicare come, nei fatti, sia l&apos;azione di iscrizione alla gestione separata ad iscrivere il lavoratore a questa gestione previdenziale.</rdfs:comment>
        <rdfs:comment xml:lang="en">This object property links together the Spilt Scheme Registration class and the Spilt Scheme class. Once applied to the Scheme you become an effective Spilt Scheme member.</rdfs:comment>
        <rdfs:label xml:lang="it">enrolApplicantInSpiltScheme</rdfs:label>
        <rdfs:label xml:lang="it">iscriveLavoratoreInGestioneSeparata</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/versatiInGestioneSeparata -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/versatiInGestioneSeparata">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/social-safety-net/ContributoGestioneSeparata"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/GestioneSeparata"/>
        <rdfs:comment xml:lang="it">La proprietà esprime il collegamento tra la classe Contributi gestione separata e  la classe Gestione separata, con lo scopo di formalizzare il legame tra questa categoria di contributo previdenziale obbligatorio e il fondo pensionistico nel quale vengono versati</rdfs:comment>
        <rdfs:comment xml:lang="en">This object property links together the Spilt Scheme Contributions class and the Spilt Scheme class. It refers to the fact that once registered a member pays their contributions into the Spilt Scheme. Contributions are used to fund social welfare benefits and pensions.</rdfs:comment>
        <rdfs:label xml:lang="en">hasContributionsPaidIntoSpiltScheme</rdfs:label>
        <rdfs:label xml:lang="it">versatiInGestioneSeparata</rdfs:label>
    </owl:ObjectProperty>



