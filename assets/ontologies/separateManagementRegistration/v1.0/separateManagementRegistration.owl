    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/PEC -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/PEC">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparata"/>
        <rdfs:comment xml:lang="en">PEC is a type of certified email system. This is one of the compulsory requirements that a beneficiary of the Spilt Scheme would have to fill in to complete their Application Form. </rdfs:comment>
        <rdfs:comment xml:lang="it">Posta Elettronica Certificata. Indirizzo indicato nella domanda di iscrizione alla gestione separata come domicilio digitale del beneficiario della domanda di iscrizione stessa</rdfs:comment>
        <rdfs:label xml:lang="it">PEC</rdfs:label>
        <rdfs:label xml:lang="en">certifiedEmail(PEC)</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/cellulare -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/cellulare">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparata"/>
        <rdfs:comment xml:lang="en">Beneficiary of the Spilt Scheme&apos;s mobile number is required to complete the Application Form. </rdfs:comment>
        <rdfs:comment xml:lang="it">Numero di telefonia mobile indicato nella domanda di iscrizione alla gestione separata come contatto utile a contattare il beneficiario della domanda di iscrizione alla gestione separata</rdfs:comment>
        <rdfs:label xml:lang="it">cellulare</rdfs:label>
        <rdfs:label xml:lang="en">mobileNumber</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/codiceAttivitaAteco -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/codiceAttivitaAteco">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparataLiberoProfessionista"/>
        <rdfs:comment xml:lang="en">ATECO code is used to identify  a self employed person&apos;s areas of work. </rdfs:comment>
        <rdfs:comment xml:lang="it">Codice identificativo dell&apos;attività svolta dal lavoratore secondo la classificazione ATECO</rdfs:comment>
        <rdfs:label xml:lang="en">ATECOCode</rdfs:label>
        <rdfs:label xml:lang="it">codice attivita ATECO</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/dataInizioAttivita -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/dataInizioAttivita">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparata"/>
        <rdfs:comment xml:lang="it">Data nella quale il lavoratore ha iniziato a svolgere l&apos;attività per la quale si rende necessaria la sua iscrizione alla gestione separata.</rdfs:comment>
        <rdfs:comment xml:lang="en">Effective date refers to the date or time when a self employed person or a dependent contractor are legally required to register with the Spilt Scheme due to the fact that they have started their job. </rdfs:comment>
        <rdfs:label xml:lang="it">dataInizioAttività</rdfs:label>
        <rdfs:label xml:lang="it">effectiveDate</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/dataPresentazioneIscrizioneGestioneSeparata -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/social-security/onto/separateManagementRegistration/dataPresentazioneIscrizioneGestioneSeparata">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/separateManagementRegistration/DomandaIscrizioneGestioneSeparata"/>
        <rdfs:comment xml:lang="it">Data nella quale la domanda di iscrizione alla gestione separata è stata presentata. La domadna di iscrizione deve essere presentata entro 30 giorni dall&apos;inizio dell&apos;attività lavorativa</rdfs:comment>
        <rdfs:comment xml:lang="en">The printout of the Spilt Scheme application form must show the date and time that the application was sent. Apllicants must send their application form within 30 days of their effective date.</rdfs:comment>
        <rdfs:label xml:lang="it">dataPresentazioneIscrizioneGestioneSeparata</rdfs:label>
        <rdfs:label xml:lang="en">spiltSchemeApplicationFormDate</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/italia/social-security/onto/separateManagementRegistration/email -->

