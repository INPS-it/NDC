        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">Esprime la modalità con cui viene pagato un Bollettino Di Pagamento. </rdfs:comment>
        <rdfs:comment xml:lang="en">Expresses the method by which a payment slip is paid.</rdfs:comment>
        <rdfs:label xml:lang="it">Modalità Di Pagamento</rdfs:label>
        <rdfs:label xml:lang="en">Payment Mode</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/italia/onto/OP21/MovimentoDiDenaro -->

    <owl:Class rdf:about="https://w3id.org/italia/onto/OP21/MovimentoDiDenaro">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/OP21/inviaRicevutaDiMovimento"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/OP21/movimentoAgisceSu"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/OP21/esegueAccredito"/>
