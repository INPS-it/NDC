### Classes

INPSSafety:EstrattoContoContributivo a owl:Class .

INPSSafety:LiberoProfessionista a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INPSSEP:versaContributiPrevidenzialiTramite ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty INPSSEP:partecipaAStudioAssociato ;
            owl:someValuesFrom owl:Thing ] .

INPSSEP:AvvisoDiAddebito a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INPSSEP:consenteInvioDomandaDiAnnullamento ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INPSSEP:consenteInvioDomandaSospensione ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INPSSEP:consultabileTramiteNotifica ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INPSSEP:servizioHaDestinatario ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Avviso di addebito"@it,
        "Debit notice"@en ;
    rdfs:comment "La classe descrive l'avviso di addebito ossia le somme da corrispondere ad INPS"@it,
        "The class describes the debit notice, i.e. the sums to be paid to INPS"@en .

INPSSEP:CassettoPrevidenzialeLiberoProfessionista a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf (
                    INPSSEP:AvvisoDiAddebito
                    INPSSEP:PartecipazioneStudioAssociato
                    INPSSEP:PosizioneDebitoriaCreditoria
                    INPSSEP:Ricorso
                    INPSSP:DelegaRilasciataLiberoProfessionista
                    INPSSP:DomandaTelematicaLiberoProfessionista
                    INPSSP:PosizioneAssicurativaLiberoProfessionista ) ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INPSSEP:utilizzaServiziCassettoPrevidenzialeLiberoProfessionista ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INPSSP:consultaCassettoPrevidenzialeLiberoProfessionista ] ;
            owl:someValuesFrom owl:Thing ] .

INPSSEP:Compensazione a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INPSSEP:compensazioneRichiestaTramiteInvioDomanda ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Compensation"@en,
        "Compensazione"@it ;
    rdfs:comment "Compensation Request. In the ontology, the class represents one of the possible types of requests presented by sending a request for reimbursement of undue contributions"@en,
        "Domanda di Compensazione. Nell'ontologia la classe rappresenta una delle possibili tipologie di istanze presentate tramite l'invio di una domanda di rimborso dei contributi non dovuti"@it .

INPSSEP:ContoBancario a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INPSSEP:codiceIBAN ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
