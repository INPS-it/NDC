    <!-- https://w3id.org/italia/social-security/onto/domesticWorkServices/EsitoValutazioneAutomaticaConRilevazioneRischio -->

    <owl:Class rdf:about="https://w3id.org/italia/social-security/onto/domesticWorkServices/EsitoValutazioneAutomaticaConRilevazioneRischio">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/social-security/onto/domesticWorkServices/EsitoValutazioneAutomatica"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/social-security/onto/domesticWorkServices/esitoValutazioneAutomaticaConRilevazioneDelRischioGenera"/>
