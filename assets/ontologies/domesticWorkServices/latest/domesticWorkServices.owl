    <!-- https://w3id.org/italia/social-security/onto/domesticWorkServices/agenteConsultaEstrattoContributivo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/domesticWorkServices/agenteConsultaEstrattoContributivo">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/italia/social-security/onto/contributions/DatoreDiLavoroDomestico"/>
