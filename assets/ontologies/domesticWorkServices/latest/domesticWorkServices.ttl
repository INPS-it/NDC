        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INPSDWS:modificaRapportoDiLavoroDomestico ] ;
            owl:someValuesFrom owl:Thing ] ;
    owl:disjointWith INPSDWS:RapportoDiLavoroDomesticoInVerifica .

INPSContributions:RapportoDiLavoroDomesticoCessato a owl:Class ;
