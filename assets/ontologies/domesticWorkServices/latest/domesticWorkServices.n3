  ], [
    a owl:Restriction ;
    owl:onProperty [ owl:inverseOf <https://w3id.org/italia/social-security/onto/domesticWorkServices/effettuaIscrizioneRapportoDiLavoroDomestico> ] ;
    owl:someValuesFrom owl:Thing
  ], [
    a owl:Restriction ;
    owl:onProperty [ owl:inverseOf <https://w3id.org/italia/social-security/onto/domesticWorkServices/raccoglieIscrizioneRapportoDiLavoroDomestico> ] ;
