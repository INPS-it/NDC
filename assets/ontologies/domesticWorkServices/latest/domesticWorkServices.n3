    a owl:Restriction ;
    owl:onProperty <https://w3id.org/italia/social-security/onto/domesticWorkServices/iscriveRapportoDiLavoroDomestico> ;
    owl:someValuesFrom owl:Thing
  ], [
    a owl:Restriction ;
