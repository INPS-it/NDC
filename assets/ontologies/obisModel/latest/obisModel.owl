    <!-- https://w3id.org/italia/onto/TI/atTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/TI/atTime">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/pensionProspectusLavoratore/ImportoPensione"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/ObisModel/TimeInterval"/>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/ObisModel/accedeA -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/ObisModel/accedeA">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/pensions/TitolareDiPensione"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/ObisModel/FascicoloPrevidenzialeCittadino"/>
        <rdfs:comment xml:lang="it">La object property collega il Titolare della pensione con il Fascicolo previdenziale del cittadino, ossia il servizio INPS che consente la consultazione del modello Obis</rdfs:comment>
        <rdfs:comment xml:lang="en">The object property connects the Pension Holder with the citizen&apos;s Social Security Record, that is the INPS service that allows the consultation of the Obis model</rdfs:comment>
        <rdfs:label xml:lang="it">accede a</rdfs:label>
        <rdfs:label xml:lang="en">access to</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/ObisModel/certificaDatiPensione -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/ObisModel/certificaDatiPensione">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/ObisModel/CertificatoDiPensione"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/italia/social-security/onto/ObisModel/DatiFiscali"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/social-security/onto/pensionProspectusLavoratore/ImportoPensione"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">La object propety collega il Certificato di pensione con le classi rappresentative dei dati che il documento stesso èutile a certificare, ossia l&apos;Importo della pensione erogato eil dettaglio dei dati fiscali</rdfs:comment>
        <rdfs:comment xml:lang="en">The object property connects the Pension Certificate with the representative classes of the data that the document itself is useful to certify, i.e. the amount of the pension paid and the details of the fiscal data</rdfs:comment>
        <rdfs:label xml:lang="it">certifica dati di pensione</rdfs:label>
        <rdfs:label xml:lang="en">certify pension data</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/ObisModel/daAccessoA -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/ObisModel/daAccessoA">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/ObisModel/FascicoloPrevidenzialeCittadino"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/social-security/onto/ObisModel/CertificatoDiPensione"/>
        <rdfs:comment xml:lang="it">La object property collega il Fascicolo previdenziale del cittadino al Certificato di pensione </rdfs:comment>
        <rdfs:comment xml:lang="en">The object property links the Citizen&apos;s social security file to the Pension Certificate</rdfs:comment>
        <rdfs:label xml:lang="it">da accesso a</rdfs:label>
        <rdfs:label xml:lang="en">gives access to</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/ObisModel/presentaDatiRelativiA -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/ObisModel/presentaDatiRelativiA">
        <rdfs:domain rdf:resource="https://w3id.org/italia/social-security/onto/ObisModel/CertificatoDiPensione"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/italia/social-security/onto/core/SedeINPS"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/social-security/onto/pensions/ProdottoPensionistico"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/social-security/onto/pensions/TitolareDiPensione"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">La object property collega il Certificato di pensione al gruppo di dati presenti nel documento, ossia quelli relativi al titolare del trattamento pensionistico, alla sede INPS corrispondente al soggetto pagatore e quelli inerenti al prodotto pensionistico erogato</rdfs:comment>
        <rdfs:comment xml:lang="en">The object property links the Pension Certificate to the group of data present in the document, i.e. those relating to the pension holder, the INPS office corresponding to the paying entity and those relating to the pension product provided</rdfs:comment>
        <rdfs:label xml:lang="it">presenta dati relativi a</rdfs:label>
        <rdfs:label xml:lang="en">presents data relating to</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/social-security/onto/pensions/haTipoDiPensione -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/social-security/onto/pensions/haTipoDiPensione">
