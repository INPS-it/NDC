        [ a owl:Restriction ;
            owl:onProperty INPSRL:stimaPercentualeAnnualePianoLavoroPrevistoEstero ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INPSRL:haPianoDiLavoroEstero ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Expected Abroad Work Plan"@en,
        "Piano lavoro previsto estero"@it ;
    rdfs:comment "Piano che descrive l'attività lavorativa che ci si aspetta di svolgere in un Paese estero. Questo include una stima in termini di tempo e reddito di suddetta attività lavorativa."@it,
        "Plan that describes the working activity expected to be carried out in a foreign Country. This plan include a time and income estimate of the working activity. "@en .

INPSRL:Pratica a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INPSRL:praticaGestitaDaSedeINPS ;
